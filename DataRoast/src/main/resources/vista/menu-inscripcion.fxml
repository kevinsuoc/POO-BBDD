<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" spacing="20.0" style="-fx-background-color: #90D5FF;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.ControladorInscripcion">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button mnemonicParsing="false" onAction="#onAgregarInscripcion" prefWidth="200.0" text="Nueva inscripcion">
      <font>
         <Font size="16.0" />
      </font></Button>
    <Button mnemonicParsing="false" onAction="#onEliminarInscripcion" prefWidth="200.0" text="Eliminar inscripcion">
      <font>
         <Font size="16.0" />
      </font></Button>
    <Button mnemonicParsing="false" onAction="#onMostrarInscripcionesFiltradas" prefWidth="200.0" text="Ver inscripciones">
      <font>
         <Font size="16.0" />
      </font></Button>
    <Button id="salir" mnemonicParsing="false" onAction="#salir" prefWidth="200.0" text="Volver">
      <font>
         <Font size="16.0" />
      </font></Button>
</VBox>
